<!-- Overlay coordinates WWI Marines Divisions -->

<kml xmlns="http://www.opengis.net/kml/2.2">
	<Document>
		<name>WWI POW-MIA Marine Corp Divisions</name>
		<description>Marine Corp Divisions overlay of occupied countries WWI</description>

		<!-- Style for map markers (US Marine Corp) -->
		<Style id="marine-corp">
			<IconStyle>
				<scale>1.1</scale>
				<Icon>
					<href>
						../images/branches/200px-USMC_logo.svg.png
					</href>
				</Icon>
			</IconStyle>
		</Style>

		<!-- Marker for US Marine Corp @  -->
		<Placemark id="wwi-us-marine-corp">
			<name><![CDATA[WWI - US Marine Corp]]></name>
				<styleUrl>#marine-corp</styleUrl>
				<description><![CDATA[
				<table>
					<tr>
						<td><img src="images/branches/200px-USMC_log.svg.png" width="50"/></td>
						<td><b>WWI - US Marine Corp</b></td>
					</tr>
					<tr>
						<td valign="top"><i>Date(s):</i></td>
						<td></td>
					</tr>
					<tr>
						<td valign="top"><i>POW:</i></td>
						<td>Unknown</td>
					</tr>
					<tr>
						<td valign="top"><i>MIA:</i></td>
						<td>Unknown</td>
					</tr>
					<tr>
						<td colspan="2"></td>
					</tr>	
					<tr>
						<td valign="top"><i>Source(s):</i></td>
						<td><a href="" target="_blank">#</a></td>
					</tr>
				</table>
				]]></description>
			<Point>
				<altitudeMode>clampedToGround</altitudeMode>
				<coordinates>
					
				</coordinates>
			</Point>
		</Placemark>

   </Document>
</kml>