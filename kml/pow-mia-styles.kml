<!-- KML stylesheet for overlays & markers -->

<!-- Style for coordinate fill(s) -->
<Style id="style">
    <LineStyle>
        <color>50FFFFFF</color>
        <width>1</width>
    </LineStyle>
    <PolyStyle>
        <color>501400E6</color>
        <fill>1</fill>
        <outline>1</outline>
    </PolyStyle>
</Style>

<!-- Style map marker for Sweden -->
<Style id="sweden">
    <IconStyle>
        <scale>1.1</scale>
        <Icon>
            <href>http://dev.in-my-cloud.com/pow-mia/images/countries/Sweden32.png</href>
        </Icon>
    </IconStyle>
</Style>

